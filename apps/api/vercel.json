{
  "version": 2,
  "builds": [
    {
      "src": "src/vercel.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "../../packages/crypto/package.json",
          "../../packages/crypto/dist/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/vercel.ts"
    }
  ]
}
